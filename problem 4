import java.util.Scanner.*;
public class Main{
 public static void main (String[] args) {
     Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int temp;
     int u=sc.nextInt();
     int p[]=new int[n];
     for(int i=0;i<n;i++)
     {
         p[i]=sc.nextInt;
         
     }
     for(int i=0;i<n;i++)
     if(p[i]==u)
     {
        System.out.println(-1);
     }
     else
     System.out.println("element:"+i);
     
 }
}
